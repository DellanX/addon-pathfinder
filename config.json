{
  "name": "Pathfinder",
  "version": "0.0.0.19",
  "slug": "pathfinder",
  "description": "Pathfinder for EVE Online!",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]",
  "ingress": true,
  "ingress_port": 1337,
  "panel_icon": "mdi:map",
  "startup": "system",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "map": ["ssl"],
  "ports": {
    "80/tcp": null
  },
  "ports_description": {
    "80/tcp": "Web interface (Not required for Ingress)"
  },
  "options": {
    "ssl": true,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str"
  }
}
